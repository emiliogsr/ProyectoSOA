<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
  xmlns:tns="http://perfil.com/"
  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  name="Perfil"
  targetNamespace="http://perfil.com/">

  <wsdl:types>
    <xsd:schema targetNamespace="http://perfil.com/">
      <xsd:include schemaLocation="perfil.xsd"/>
    </xsd:schema>
  </wsdl:types>

  <wsdl:message name="consultarRequest">
    <wsdl:part name="parameters" element="tns:consultarPerfil"/>
  </wsdl:message>
  <wsdl:message name="consultarResponse">
    <wsdl:part name="parameters" element="tns:resultadoPerfil"/>
  </wsdl:message>

  <wsdl:portType name="PerfilPort">
    <wsdl:operation name="consultar">
      <wsdl:input message="tns:consultarRequest"/>
      <wsdl:output message="tns:consultarResponse"/>
    </wsdl:operation>
  </wsdl:portType>

  <wsdl:binding name="PerfilSOAP" type="tns:PerfilPort">
    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="consultar">
      <soap:operation soapAction="http://perfil.com/consultar"/>
      <wsdl:input><soap:body use="literal"/></wsdl:input>
      <wsdl:output><soap:body use="literal"/></wsdl:output>
    </wsdl:operation>
  </wsdl:binding>

  <wsdl:service name="PerfilService">
    <wsdl:port name="PerfilPort" binding="tns:PerfilSOAP">
      <soap:address location="http://localhost:8082/serviciosdigitales/perfil"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>
